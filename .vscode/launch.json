    "version": "0.2.0",
    "configurations": [

        { 
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "${workspaceFolder}/output/main",
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Main EJ1",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Resolucion/bin/ej1",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build main1"
        },
        {
            "name": "(gdb) Main EJ2",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/Resolucion/bin/ej2",
            "stopAtEntry": false,
            "args": ["/home/ezequiel/Documents/GitRepos/Facu/Algo3_2019c1/tp2/Algo3-TP2/Experimentacion/datos_entrada/ej2/input1.txt"],
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build main2"
        }
    ]
}